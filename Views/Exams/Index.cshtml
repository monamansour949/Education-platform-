@model IEnumerable<ProjectItiTeam.Models.Exam>

@{
    Layout="_ExamLayout";
}



<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.Date)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Degree)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Mcq_Num)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Tf_Num)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Course_Id)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Date)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Degree)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Mcq_Num)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Tf_Num)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Course_Id)
            </td>
          <td>
                <a asp-action="Edit" class="btn btn-success" asp-route-id="@item.Id">Edit
                    <i class="fa fa-edit"></i>
                </a> |
                <a asp-action="Details" class="btn btn-primary" asp-route-id="@item.Id">Details
                    <i class="fa fa-info"></i>
                </a> |
                <a asp-action="Delete" class="btn btn-danger" asp-route-id="@item.Id">Delete
                    <i class="fa fa-trash"></i>
                </a>
            </td>
        </tr>
}
    </tbody>
</table>

 <a class="btn btn-outline-light rounded-pill w-50 me-25" asp-action="Create">Create</a>
